<%= form_tag('/collaborations') do %>
<%= hidden_field_tag :wiki_id, @wiki_id %>
<div class="row">
  <div class="col-md-5">
    <h2>List of Collaborators</h2>
    <% @user.each do |u| %>
      <div class="media">
        <div class="media-body">
          <h4 class="pull-left">
            <%= u.email %>
            <%= check_box_tag 'selected_emails[]', u.email -%>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
</div>
<%= submit_tag %>
<% end %>
