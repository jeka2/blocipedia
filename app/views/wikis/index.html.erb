<h1>List of Wiki Articles</h1>

<%= link_to "Create an article", @wiki, method: :new, class: 'btn btn-info' %>
<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |wiki| %>
    <div class="media">
      <div class="media-body">
        <h4 class="media-heading">
          <%= link_to wiki.title, wiki %>
        </h4>
      </div>
    </div>
    <% end %>
  </div>
</div>
